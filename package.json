{
    "name": "stone",
    "displayName": "S.T.O.N.E.",
    "description": "Send To OneNote Extension for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "wonsong",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sendPageToOneNote",
        "onCommand:extension.sendSelectionToOneNote"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sendPageToOneNote",
                "title": "Send this page to OneNote"
            },
            {
                "command": "extension.sendSelectionToOneNote",
                "title": "Send the selection to OneNote"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "@types/express": "^4.0.36",
        "@types/markdown-it": "0.0.2",
        "express": "^4.15.3",
        "highlightjs": "^9.10.0",
        "markdown-it": "^8.3.1",
        "opn": "^5.1.0",
        "request": "^2.81.0"
    }
}
